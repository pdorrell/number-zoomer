{
  "name": "number-zoomer",
  "version": "1.0.0",
  "description": "Interactive X-Y coordinate plane for exploring decimal precision",
  "main": "dist/index.js",
  "scripts": {
    "start": "webpack serve --mode development --config webpack.config.user.js",
    "start:claude": "webpack serve --mode development --config webpack.config.claude.js",
    "build": "webpack --mode production",
    "type-check": "tsc --noEmit",
    "check-code": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build:full": "npm run check-code && npm run test && npm run build",
    "deploy": "node scripts/update-version.js && npm run build:full && node scripts/deploy-to-target.js"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^30.0.5",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "ts-jest": "^29.4.0",
    "ts-loader": "^9.5.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@use-gesture/react": "^10.3.1",
    "decimal.js": "^10.4.3",
    "mobx": "^6.10.2",
    "mobx-react-lite": "^4.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
